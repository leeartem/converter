{
    "name": "leeartem/converter",
    "description": "Lightweight currency converter library for Laravel.",
    "license": "MIT",
    "authors": [
        {
            "name": "Lee Artem",
            "email": "lee.tyoma@gmail.com",
            "homepage": "https://artemlee.ru"
        }
    ],
    "homepage": "https://github.com/leeartem/converter",
    "keywords": ["Laravel", "Converter"],
    "require": {
        "php": ">=7.0",
        "illuminate/support": "~9",
        "illuminate/cache": "^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~7"
    },
    "autoload": {
        "psr-4": {
            "LeeArtem\\Converter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LeeArtem\\Converter\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LeeArtem\\Converter\\ConverterServiceProvider"
            ],
            "aliases": {
                "Converter": "LeeArtem\\Converter\\Facades\\Converter"
            }
        }
    }
}
